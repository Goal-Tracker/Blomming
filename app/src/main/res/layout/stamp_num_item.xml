            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/three_memeber3_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginTop="13dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/three_memeber2_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginEnd="13dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/three_memeber1_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <!-- 네 명 한 경우 -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/four_stamp_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="visible">

        <View
            android:id="@+id/four_stampNum_view"
            android:layout_width="46dp"
            android:layout_height="46dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/four_memeber3_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginEnd="13dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/four_memeber4_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginTop="12dp"
            android:layout_marginStart="13dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/four_memeber2_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginEnd="13dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/four_memeber1_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="13dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <!-- 다섯 명 한 경우 -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/five_stamp_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="gone">

        <View
            android:id="@+id/five_stampNum_view"
            android:layout_width="46dp"
            android:layout_height="46dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/five_memeber4_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/five_memeber3_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginEnd="11dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/five_memeber2_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginEnd="13dp"
            android:layout_marginTop="22dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/five_memeber1_view"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="13dp"
            android:layout_marginTop="22dp"
            android:background="@drawable/user_profile_circle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/five_memeber5_view"
            android:layout_width="12dp"
